section.newest-transaction
    .container
        .row
            .col-md-12
                .newest-transaction__title Newest Transaction
        .row
            each classname, name in {PH: "request--ph", GH: "request--gh"}
                .col-md-6.padding-0
                    .request(class=classname)
                        .row
                            .col-md-12.request__title 
                                |Newest 
                                span=name
                                |  Requests
                        .row.request__header
                            .col-xs-5.col-md-5 Username
                            .col-xs-2.col-md-2 Amount
                            .col-xs-5.col-md-5 Date
                        each val in [1,2,3,4,5] 
                            .row.request__item
                                .col-xs-5.col-md-5 hoangnguyen
                                .col-xs-2.col-md-2 3
                                .col-xs-5.col-md-5 10/08/2016